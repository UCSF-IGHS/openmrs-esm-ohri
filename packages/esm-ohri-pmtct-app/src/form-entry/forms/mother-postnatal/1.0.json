                    "label": "Visit Date",
                    "type": "obs",
                    "id": "visit_date",
                    "questionOptions": {
                      "concept": "163260AAAAAAAAAAAAAAAAAAAAAAAAAAAAAA",
                      "rendering": "date"
                    },
                    "behaviours":[
                      {
                        "intent":"*",
                        "required":"true",
                        "unspecified":"true",
                        "hide":{
                          "hideWhenExpression":"false"
                        },
                        "validators": [
                          {
                            "type":"date",
                            "allowFutureDates":"false"
                          },
                          {
                            "type":"js_expression",
                            "failsWhenExpression":"isDateBefore(myValue, '1980-01-01') || myValue >= today()"
                          }
                        ]
                      }
                    ]
                  },
                  {
                      "label":"PTracker Id",
                      "type":"obs",
                      "id":"ptrackerid",
                      "questionOptions":{
                         "concept":"6c45421e-2566-47cb-bbb3-07586fffbfe2",
                         "rendering":"text"
                      },
                      "behaviours":[
                        {
                          "intent":"*",
                          "required":"true",
                          "hide":{
                            "hideWhenExpression":"false"
                          },
                          "validators":[
                           {
                              "type": "js_expression",
                              "failsWhenExpression": "!/^[0-9]{5}[A-S]{1}[0-9]{6,8}$$/.test(myValue)",
                              "message": "Invalid Ptracker number"
                            }

                          ]
                        }
                      ]
                   }
                ]
             }
